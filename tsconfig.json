{
    "compilerOptions": {
      "target": "ES2020",
      "module": "ESNext",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "outDir": "./dist"
    },
    "include": ["src"],
    "typedocOptions": {
      "entryPoints": ["./src"],
      "entryPointStrategy": "expand",
      "out": "./_docs",
      "readme": "README.md",
      "projectDocuments": "./docs",
      "theme": "default",
      "excludeNotDocumented": true,
      "hideGenerator": true,
      "cleanOutputDir": true,
      "plugin": [
        "typedoc-plugin-mdn-links",
        "@vpalmisano/typedoc-plugin-ga",
        "@vpalmisano/typedoc-cookie-consent"
      ],
      "gaID": "G-05SF29M4QB",
      "gaProperties": {
        "type": "text/plain",
        "data-category": "analytics"
      },
      "cookieConsent": {
        "enabled": true,
        "showPreferencesBtn": true
      }
    }
  }